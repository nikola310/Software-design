package editor;

import java.awt.event.ActionEvent;
import java.awt.event.ActionListener;

/**
 * Akcija koja instancira komandu za otvaranje editora seme.
 * 
 * @author Nikola Stojanovic
 */
public class OtvaranjeEditoraAkcija implements ActionListener {

	@Override
	public void actionPerformed(ActionEvent arg0) {
		OtvaranjeEditoraKomanda oek = new OtvaranjeEditoraKomanda();
		oek.izvrsi();
	}

}
